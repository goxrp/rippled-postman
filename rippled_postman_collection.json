                        {
                            "name": "ledger_current",
                            "request": {
                                "url": {
                                    "raw": "{{RIPPLED_JSONRPC_URL}}",
                                    "host": [
                                        "{{RIPPLED_JSONRPC_URL}}"
                                    ]
                                },
                                "method": "POST",
                                "description": "The ledger_current method returns the unique identifiers of the current in-progress [ledger](https://xrpl.org/ledgers.html). This command is mostly useful for testing, because the ledger returned is still in flux.",
                                "body": {
                                    "mode": "raw",
                                    "raw": "{\n\t\"method\": \"ledger_current\",\n\t\"params\": [\n\t\t{}\n\t]\n}"
                                }
                            }
                        },
                        {
                            "name": "ledger_data",
                            "request": {
                                "url": {
                                    "raw": "{{RIPPLED_JSONRPC_URL}}",
                                    "host": [
                                        "{{RIPPLED_JSONRPC_URL}}"
                                    ]
                                },
                                "method": "POST",
                                "description": "The `ledger_data` method retrieves contents of the specified ledger. You can iterate through several calls to retrieve the entire contents of a single ledger version.",
                                "body": {
                                    "mode": "raw",
                                    "raw": "{\n\t\"method\": \"ledger_data\",\n\t\"params\": [\n\t\t{\n\t\t\t\"binary\": false,\n\t\t\t\"ledger_hash\": \"87B7E3C234DB58104751F46F54262A85B2B8AD0C31982EC71AFF5CAE4511F91C\",\n\t\t\t\"limit\": 5\n\t\t}\n\t]\n}"
